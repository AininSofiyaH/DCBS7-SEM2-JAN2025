package tasklooping5;

/**
 *
 * @author sufi
 */
public class TaskLooping5 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        System.out.println("Number sequence: "); //header
        
        //  loop to print num from 99 to 11, decreasing by 11 easch time
        for (int j = 99; j >= 11; j -= 11){
            System.out.print(j + " "); // print num on the same line
        }
    }
    
}

public class TaskForToday_5 {
    public static void main(String[] args) {

        //chapter 4 (task 5)
        //for loop
        
        int count, gandaan, sum;                        //declare dulu
        
        System.out.println("for loop");
        System.out.println("Nombor gandaan 11");{

        count = 11;                                     //samakan dgn 11 supaya dia start kira dkt 11
        for(count = 11; count <= 100; count += 11){     //baca semua syntax dulu baru print pastu baru ulang 
            
            sum = count + 11;                           //formula nk cari gandaan 11 
            System.out.println(count);                  //count sbb kite simpan nom tu dlm count yg (count += 11)
             
        }}                              
     
       System.out.println("\n");
       
    }
    
}
